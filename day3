class Solution
{
    public int LongestRepeatingSubsequence(String str)
    {
        int n=str.length();
        int dp[][]=new int[str.length()+1][str.length()+1];
        for(int i=n-2;i>=0;i--)
        {
            for(int j=dp[0].length-2;j>=0;j--)
            {
                if(str.charAt(i)==str.charAt(j) && i!=j)
                {
                    dp[i][j]=1+dp[i+1][j+1];
                }
                else
                {
                    dp[i][j]=Math.max(dp[i+1][j],dp[i][j+1]);
                }
            }
        }
        return dp[0][0];
    }
}
